<resources xmlns:xliff="http://schemas.android.com/tools">
    <string name="app_name">UploadManager</string>

    <string name="upload_unknown_upload_title">未知</string>
    <string name="upload_no_application_title">无可打开此文件的应用</string>
    <string name="upload_remaining">剩余
        <xliff:g id="DURATION" example="3 minutes">%s</xliff:g>
    </string>
    <string name="notification_waiting_for_suitable_network">等待重试或合适的网络连接</string>
    <string name="notification_upload_successfully">上传成功</string>
    <string name="notification_upload_unsuccessfully">上传失败</string>
    <string name="notification_upload_file_not_found">文件不存在</string>
    <string name="notification_action_redo"></string>
    <string name="notification_action_cancel"></string>
    <string name="delete_confirm_dialog_title">确认删除？</string>
    <string name="delete_confirm_dialog_message">所选中的文件中，已下载文件将被删除，上传的文件将被保留。</string>
    <string name="delete_confirm_positive_button">确认</string>
    <string name="delete_confirm_negative_button">取消</string>
    <plurals name="notif_summary_active">
        <item quantity="one">1 个文件正在上传</item>
        <item quantity="other"><xliff:g id="number">%d</xliff:g> 个文件在上传</item>
    </plurals>
    <plurals name="notif_summary_waiting">
        <item quantity="one">1 个文件在等待上传</item>
        <item quantity="other"><xliff:g id="number">%d</xliff:g> 个文件在等待上传</item>
    </plurals>
    <plurals name="notif_summary_failed">
        <item quantity="one">1 个文件上传失败</item>
        <item quantity="other">
            <xliff:g id="number">%d</xliff:g>
            个文件上传失败
        </item>
    </plurals>

    <plurals name="duration_seconds">
        <item quantity="one">1 秒</item>
        <item quantity="other">
            <xliff:g id="count">%d</xliff:g>
            秒
        </item>
    </plurals>
    <!-- Phrase describing a time duration using minutes [CHAR LIMIT=16] -->
    <plurals name="duration_minutes">
        <item quantity="one">1 分钟</item>
        <item quantity="other">
            <xliff:g id="count">%d</xliff:g>
            分钟
        </item>
    </plurals>
    <!-- Phrase describing a time duration using hours [CHAR LIMIT=16] -->
    <plurals name="duration_hours">
        <item quantity="one">1 小时</item>
        <item quantity="other">
            <xliff:g id="count">%d</xliff:g>
            小时
        </item>
    </plurals>
</resources>
